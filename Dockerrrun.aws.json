{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [{
		"name": "client",
		"image": "gloompi/multi-client",
		"hostname": "client",
		"essential": false
	}, {
		"name": "server",
		"image": "gloompi/multi-server",
		"hostname": "api",
		"essential": false
	}, {
		"name": "worker",
		"image": "gloompi/multi-worker",
		"essential": false
	}, {
		"name": "nginx",
		"image": "gloompi/multi-nginx",
		"essential": true,
		"portMappings": [{
			"hostPort": 80,
			"containerPort": 80
		}],
		"links": ["client", "server"]
	}]
}